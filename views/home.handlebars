<h1>{{title}}</h1>
<ul>
    {{#each products}}
    <li>
        <h3>{{title}}</h3>
        <p>{{description}}</p>
        <p><strong>Precio:</strong> ${{price}}</p>
        <p><strong>Stock:</strong> {{stock}}</p>
        <p><strong>Categor√≠a:</strong> {{category}}</p>
        {{#if thumbnails}}
        <img src="{{thumbnails.[0]}}" alt="{{title}}" style="width:100px;">
        {{/if}}
    </li>
    {{/each}}
</ul>
